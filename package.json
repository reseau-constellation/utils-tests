{
  "version": "0.2.3",
  "license": "AGPL-3.0-or-later",
  "name": "@constl/utils-tests",
  "author": "Julien Jean Malard-Adam",
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "exports": "./dist/src/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "nettoyer": "rimraf dist",
    "compiler": "pnpm versioner && pnpm formatter && pnpm nettoyer && pnpm tspc -p tsconfig.build.json",
    "test:node": "pnpm compiler && pnpm aegir test -t node --cov -- --exit",
    "test:navig": "pnpm compiler && pnpm aegir test -t browser --cov",
    "test:électron-principal": "pnpm compiler && pnpm aegir test -t electron-main --cov",
    "test:électron-rendu": "pnpm compiler && pnpm aegir test -t electron-renderer --cov",
    "test": "pnpm test:node && pnpm test:navig",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint --ext .js,.ts src",
    "publier": "pnpm versioner && pnpm compiler && (git commit -a -m 'version' || true) && pnpm publish",
    "versioner": "pnpm genversion  --es6 --semi src/version.ts"
  },
  "watch": {
    "build": {
      "patterns": "./src/**/*",
      "extensions": [
        ".ts",
        ".tsx"
      ]
    },
    "postbuild": "./dist/**/*"
  },
  "devDependencies": {
    "@constl/ipa": "^0.9.5",
    "@types/crypto-js": "^4.1.3",
    "@types/file-saver": "^2.0.7",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.9",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.9.0",
    "aegir": "^42.0.0",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "core-js": "^3.33.1",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "file-saver": "^2.0.5",
    "mocha": "^10.2.0",
    "playwright": "^1.41.1",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "5.3.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@helia/block-brokers": "^1.0.0",
    "@orbitdb/quickstart": "^1.0.0",
    "blockstore-core": "^4.3.10",
    "blockstore-level": "^1.1.7",
    "chokidar": "^3.5.3",
    "events": "^3.3.0",
    "genversion": "^3.2.0",
    "helia": "^3.0.1",
    "libp2p": "^1.2.0",
    "rimraf": "^5.0.5",
    "uint8arrays": "^5.0.0",
    "uuid": "^9.0.1",
    "wherearewe": "^2.0.1"
  }
}
